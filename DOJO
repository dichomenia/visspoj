#include <stdio.h>
#include <math.h>
#include <string.h>
 
int main()
{
	int tc;
	scanf("%d", &tc);
	
	for(int z = 1; z <= tc; z++)
	{
		int n, i;
		char m[2005];
		char j[2005];
		scanf("%d %s %d %s", &n, &m, &i, &j);
		
		int pm = strlen(m) - 1;
		int pj = strlen(j) - 1;
		int a = n*m[pm];
		
		if(a%2 == 1 && i%2 == j[pj]%2)
			printf("Possible.\n");
		else
			printf("Impossible.\n");
	}
} 
